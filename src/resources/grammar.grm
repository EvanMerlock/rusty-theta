literal     : NUMBER | STRING | "true" | "false" | "nil" ;
expression  : equality;
equality    : comparison ( ( "!=" | "==" ) comparison )*;
comparison  : term ( ( ">" | ">=" | "<" | "<=" ) term )*;
term        : factor ( ( "-" | "+" ) factor )*;
factor      : unary ( ( "/" | "*" ) unary )*;
unary       : ( "!" | "-" ) unary
            | primary;
primary     : literal | "(" expression (";" expression)* ")";